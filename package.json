{
  "name": "arbitrary",
  "version": "0.0.6",
  "description": "Pseudo Random Number Generator",
  "main": "arbitrary.js",
  "scripts": {
    "clean": "rm -rf lib && mkdir lib",
    "test": "./node_modules/.bin/mocha tests",
    "build": "npm run clean && ./node_modules/.bin/browersify src/index.js -t [ babelify ] -o lib/index.js -p mapstraction",
    "watch": "npm run clean && ./node_modules/.bin/watchify src/index.js -t [ babelify ] -o lib/index.js -p mapstraction --debug"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/francoislaberge/arbitrary.git"
  },
  "keywords": [
    "random",
    "generator",
    "tinymt",
    "mersenne",
    "twister"
  ],
  "author": "Francois Laberge <francoislaberge@gmail.com>",
  "bugs": {
    "url": "https://github.com/francoislaberge/arbitrary/issues"
  },
  "homepage": "https://github.com/francoislaberge/arbitrary",
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "mapstraction": "^1.0.0",
    "mocha": "^2.3.4",
    "watchify": "^3.7.0"
  },
  "license": "MIT"
}
